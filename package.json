  "dependencies": {
    "chalk": "^2.3.2",
    "chrome-launcher": "^0.10.2",
    "commander": "^2.15.1",
    "fancy-log": "^1.3.2",
    "graphite-client": "^1.0.1",
    "joi": "^13.1.2",
    "lighthouse": "^2.9.4",
    "mkdirp": "^0.5.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2"
  },
  "peerDependencies": {
